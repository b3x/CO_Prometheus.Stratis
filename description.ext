		default = true;
		isGlobal = 0;
	};

	class IsStreamable
	{
		title = "enable live stream (needs 'enable replay')";
		values[] = {false, true};
		texts[] = {"no","yes"};
		default = 0;
		isGlobal = 0;
	};
	
	class Debug_Mode
	{
		title = "Enable Debug Log";
		values[] = {false, true};
		texts[] = {"no","yes"};
		default = 0;
	};


};

